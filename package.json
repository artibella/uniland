{
  "name": "uniland",
  "version": "0.8.0",
  "private": true,
  "scripts": {
    "dev": "run-s download:manifest dev:next",
    "dev:next": "next dev",
    "build": "run-s download:manifest build:next",
    "build:next": "next build",
    "start": "next start",
    "download:manifest": "uniform context manifest download --output ./src/lib/contextManifest.json",
    "component:pull": "uniform canvas component pull ./exports/component",
    "component:push": "uniform canvas component push ./exports/component",
    "composition:pull": "uniform canvas composition pull ./exports/composition",
    "composition:pull:json": "uniform canvas composition pull ./exports/composition/compositions.json",
    "composition:push": "uniform canvas composition push ./exports/composition",
    "composition:push:json": "uniform canvas composition push ./exports/compositions/compositions.json",
    "category:pull": "uniform canvas category pull ./exports/category",
    "category:push": "uniform canvas category push ./exports/category",
    "project-map-definition:push": "uniform project-map definition push -m createOrUpdate ./exports/projectMapDefinition",
    "project-map-definition:pull": "uniform project-map definition pull ./exports/projectMapDefinition",
    "project-map-node:push": "uniform project-map node push -m createOrUpdate ./exports/projectMapNode",
    "project-map-node:pull": "uniform project-map node pull ./exports/projectMapNode",
    "project-map:import": "run-s project-map-definition:push project-map-node:push",
    "project-map:export": "run-s project-map-definition:pull project-map-node:pull",
    "redirects:pull": "uniform redirect definition pull ./exports/redirect/redirects.json -f json",
    "redirects:push": "uniform redirect definition push ./exports/redirect/redirects.json -f json",
    "canvas:export": "run-s component:pull composition:pull",
    "canvas:import": "run-s component:push composition:push",
    "signal:pull": "uniform context signal pull ./exports/signal",
    "signal:push": "uniform context signal push ./exports/signal",
    "enrichment:pull": "uniform context enrichment pull ./exports/enrichment",
    "enrichment:push": "uniform context enrichment push ./exports/enrichment",
    "aggregate:pull": "uniform context aggregate pull ./exports/aggregate",
    "aggregate:push": "uniform context aggregate push ./exports/aggregate",
    "quirk:pull": "uniform context quirk pull ./exports/quirk",
    "quirk:push": "uniform context quirk push ./exports/quirk",
    "context:export": "run-s signal:pull enrichment:pull aggregate:pull quirk:pull",
    "context:import": "run-s signal:push enrichment:push aggregate:push quirk:push",
    "datatype:pull": "uniform canvas datatype pull ./exports/datatype",
    "datatype:push": "uniform canvas datatype push ./exports/datatype",
    "project:import": "run-s datatype:push context:import canvas:import project-map:import",
    "project:export": "run-s datatype:pull context:export canvas:export project-map:export",
    "uniform:pull": "uniform sync pull",
    "uniform:push": "uniform sync push",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@lottiefiles/lottie-player": "^1.6.0",
    "@uniformdev/canvas": "19.165.0",
    "@uniformdev/canvas-contentful": "19.165.0",
    "@uniformdev/canvas-contentstack": "^19.59.0",
    "@uniformdev/canvas-next": "19.165.0",
    "@uniformdev/canvas-react": "19.165.0",
    "@uniformdev/cli": "19.165.0",
    "@uniformdev/context": "19.165.0",
    "@uniformdev/context-gtag": "19.165.0",
    "@uniformdev/context-next": "19.165.0",
    "@uniformdev/context-react": "19.165.0",
    "@uniformdev/project-map": "19.165.0",
    "@uniformdev/redirect": "19.165.0",
    "classnames": "^2.3.2",
    "contentful": "^10.5.1",
    "contentstack": "^3.17.1",
    "critters": "0.0.20",
    "eslint-config-next": "^13.1.6",
    "is-html": "^3.0.0",
    "next": "14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@contentful/rich-text-html-renderer": "^16.1.1",
    "@contentful/rich-text-react-renderer": "^15.17.1",
    "@storybook/addon-actions": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-mdx-gfm": "^7.6.17",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/addon-styling-webpack": "^0.0.6",
    "@storybook/blocks": "^7.6.17",
    "@storybook/nextjs": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "daisyui": "^4.7.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-import": "^16.0.1",
    "postcss-loader": "^8.1.0",
    "prettier": "3.2.5",
    "react-daisyui": "5.0.0",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "13.0.2",
    "slugify": "1.6.6",
    "storybook": "7.6.17",
    "style-loader": "3.3.4",
    "tailwindcss": "3.4.1"
  },
  "resolutions": {
    "webpack": "^5"
  }
}